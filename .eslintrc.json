{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "new-cap": "off",
    "max-len": "off",
    "no-console": "off"
  }
}
